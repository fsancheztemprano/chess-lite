    "@jest/reporters": "27.5.1",
    "@ngneat/elf-cli": "3.0.0",
    "@ngneat/elf-cli-ng": "1.0.0",
    "@ngneat/transloco-keys-manager": "3.4.1",
    "@nrwl/cli": "14.1.5",
    "@nrwl/cypress": "14.1.5",
    "@nrwl/devkit": "14.1.5",
    "@nrwl/eslint-plugin-nx": "14.1.5",
    "@nrwl/jest": "14.1.5",
    "@nrwl/linter": "14.1.5",
    "@nrwl/node": "14.1.5",
    "@nrwl/workspace": "14.1.5",
    "@nxrocks/nx-spring-boot": "4.0.2",
    "@pact-foundation/pact": "9.17.3",
    "@schematics/angular": "14.0.1",
    "@schematics/schematics": "0.1102.12",
    "@types/jest": "27.5.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.32",
    "@types/sockjs-client": "1.5.1",
    "@types/url-template": "2.0.28",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "cypress": "9.6.1",
    "dotenv": "16.0.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-github": "4.3.6",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "jest-preset-angular": "11.1.2",
    "jsonwebtoken": "8.5.1",
    "junit-report-merger": "3.0.5",
    "ng-packagr": "13.3.1",
    "npm-check-updates": "12.5.11",
    "nx": "14.1.5",
    "prettier": "2.6.2",
    "sonarqube-scanner": "2.8.1",
