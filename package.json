    "ts-node": "10.7.0",
    "typescript": "4.6.4",
    "webpack-bundle-analyzer": "4.5.0"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "inlineStoreInClass": false,
      "idKey": "id",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ]
    }
  }
}
